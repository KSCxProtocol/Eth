<br/>
<div class='col-md-6'>
    <h3>Latest Blocks</h3>
    <SfGrid TValue="DataModal" DataSource="@Data" AllowPaging="true" GridLines="GridLine.Both">
        <GridPageSettings PageSize="10"></GridPageSettings>    
    <GridColumns>
        <GridColumn Field=@nameof(DataModal.Index) HeaderText="Index" Width="120"></GridColumn>
        <GridColumn Field=@nameof(DataModal.Transactions) HeaderText="Transactions" Width="150"></GridColumn>
        <GridColumn Field=@nameof(DataModal.DateTime) HeaderText="DateTime" Type="ColumnType.Date"Width="130"></GridColumn>
        <GridColumn Field=@nameof(DataModal.Size) HeaderText="Size(Bytes)" Width="120"></GridColumn>
    </GridColumns>
</SfGrid>
</div>
